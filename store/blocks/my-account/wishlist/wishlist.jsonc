{
    "my-account-link.wishlist-link": {
        "props": {
            "label": "My Wishlist"
        }
    },
    "my-account-page.wishlist-page": {
        "props": {
            "title": "Wishlist"
        },
        "children": [
            "list-context.wishlist"
        ]
    },
    "store.wishlist": {
        "blocks": [
            "flex-layout.row#top",
            "list-context.wishlist"
        ]
    },
    "flex-layout.row#top": {
        "children": [
            "flex-layout.col#title"
        ]
    },
    "flex-layout.col#title": {
        "children": [
            "rich-text#title"
        ],
        "props": {
            "blockClass": "titleWishlist",
            "preventVerticalStretch": true
        }
    },
    "rich-text#title": {
        "props": {
            "text": "### Wishlist"
        }
    },
    "list-context.wishlist": {
        "blocks": [
            "product-summary.shelf#default"
        ],
        "children": [
            "slider-layout#slider-layout-wishlist"
        ]
    }
}